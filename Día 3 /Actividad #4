alumnos =  {
    
    "Juan":8,
    "Giselle":9,
    "Damian":5,
    "Ricardo":6,
    "Yoatzin":6,
    "Erick":7,
    "Mario":8,
    "Edgar":9,
    "Fernanda":5,
    "Daniel":6,
    "Jesus": 7,
    "Damian": 8,
    "Yemahiba":6,
    "Eduardo": 9, 
    "Bryan": 9,
    "Mariano": 10, 
    "Alberto": 8
       
}

def promedio ():
    
    totalCalif = 0
    promedio = 0
    total = 0
    
    for calif in alumnos: 
        totalCalif = totalCalif + alumnos[calif]
        total = total + 1
    
    promedio = totalCalif/(total)
    
    
    
    
    print(promedio)
    


def mayor (): 
    print("Mejores calificaciones: ")
    for calif   in alumnos:
        if(int(alumnos[calif]) == 10):
            print(str(calif), " : ", str(alumnos[calif]))
         


def menor ():
    print ("Calificaciones m√°s bajas: ")
    for calif in alumnos:
        if (int(alumnos[calif])==5):
            print(str(calif), " : ", str(alumnos[calif]))

mayor()    
promedio()
menor()
